  "name": "handlebars-compile-plugin",
  "version": "1.0.2",
  "description": "Handlebars plugin for the YUI 3 shifter build tool.",
  "bin": {
    "handlebars-compile": "bin/handlebars-compile.js"
  },
  "main" : "./lib/index.js",
  "scripts": {
    "pretest": "jshint --config ./node_modules/yui-lint/jshint.json ./lib/",
    "test": "vows --spec tests/*.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bretkikehara/handlebars-compile-plugin.git"
  },
  "dependencies": {
    "path" : "*",
    "vows" : "*",
    "assert" : "*",
    "handlebars": "*",
    "optimist": "0.6.0",
    "yui-lint": "~0.1.1"
  },
  "preferGlobal": "true"
}